[Unit]
Description=Bridgefall Proxy Server
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
ExecStart=/usr/local/bin/paniq-proxy --config /etc/bridgefall/paniq-proxy.json --profile /etc/bridgefall/profile.json
Restart=on-failure
RestartSec=2s
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
